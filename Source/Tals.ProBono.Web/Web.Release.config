<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="SiteUrl" value="https://waweblawyer.org/" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SiteEmail" value="edmund.gorski@gmail.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>

  <location>
    <system.web>
      <compilation xdt:Transform="RemoveAttributes(debug)" />
      <customErrors mode="On" defaultRedirect="/Error.htm" xdt:Transform="Replace" />
    </system.web>
  </location>

  <elmah>
    <errorLog type="Elmah.MemoryErrorLog, Elmah" size="50" />
    <security allowRemoteAccess="1" />
  </elmah>
</configuration>